})\n\n  const {user, users, repos, loading} = state\n\n  return (\n    <GithubContext.Provider value={{\n      setLoading, search, getUser, getRepos, clearUsers, user, users, repos, loading\n    }}>\n      {children}\n    </GithubContext.Provider>\n  )\n}","/home/alexander/Desktop/Pet-projects/react-hooks/src/context/github/githubReducer.js",[],"/home/alexander/Desktop/Pet-projects/react-hooks/src/components/Repos.js",[],"/home/alexander/Desktop/Pet-projects/react-hooks/src/context/alert/alertReducer.js",[],{"ruleId":"79","replacedBy":"80"},{"ruleId":"81","replacedBy":"82"},{"ruleId":"83","severity":1,"message":"84","line":6,"column":13,"nodeType":"85","messageId":"86","endLine":6,"endColumn":16},"no-native-reassign",["87"],"no-negated-in-lhs",["88"],"no-unused-vars","'url' is defined but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation"]